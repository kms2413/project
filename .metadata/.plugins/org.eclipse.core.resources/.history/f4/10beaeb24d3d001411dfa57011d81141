/**
 * 
 */

function writeCheck(form){
	if(form.name.value==""){
		alert("이름 입력하세여~");
		form.name.focus();
		return false;
	}
	
	if(form.password.value==""){
		alert("비번 입력하세여~");
		form.password.focus();
		return false;
	}
	
	if(form.message.value==""){
		alert("내용 입력하세여~");
		form.message.focus();
		return false;
	}
	
}