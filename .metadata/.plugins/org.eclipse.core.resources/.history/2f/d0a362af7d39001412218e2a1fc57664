/**
 * 
 */

function insertForm(form){
	if(form.name.value==""){
		alert("NAME!!");
		form.name.focus();
		return false;
	}
	
	if(form.phone.value==""){
		alert("phone!!");
		form.phone.focus();
		return false;
	}
	
	if(form.email.value==""){
		alert("EMAIL!!");
		form.email.focus();
		return false;
	}
	
}